{% extends 'ISGProjectSubmissionAppBundle:Common:Unregistered/main.html.twig' %}

{% block content %}

<div class="container-lg">
 
 <div class="header-lg"> <h3>Project Submission</h3></div>
 <div class="content-lg">
 
 
 {% if error != "" %}
 <div id="errors-lg" >
  
   {{ error }}
 
 </div>
 
 
 
 {% endif %}
 
 <div class="form-lg">
<form action="{{ path('_mainIndex') }}" method="post" {{ form_enctype(form) }}>
  
  <div class="form-text-lg"> <span class="label-lg"> Username: </span> {{ form_widget(form.Username) }} </div>
  <div class="form-text-lg"> <span class="label-lg"> Password: </span>{{ form_widget(form.password) }} </div>
  {{form_rest(form)}}
  <div id="form-lg-error"> </div>
 
  <div class="form-lg-submit"> <input type="submit" name="Log In" value="Log In"/> </div>
 
 </form>
 
 </div>
 <div class="addinfo-lg">
 Here is the additional info or disclaimer positioned
 If you forgot your password proceed to click on this link
  </div>
 
 </div>
</div>




{% endblock %}